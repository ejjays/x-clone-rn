{
  "expo": {
    "name": "PCMI - Infanta",
    "slug": "pcmi-infanta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires camera access for video calls.",
        "NSMicrophoneUsageDescription": "This app requires microphone access for calls.",
        "UIBackgroundModes": ["audio", "voip"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#242526"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ejjays.mobile",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan"
    },
    "androidStatusBar": {
      "backgroundColor": "#00000000",
      "barStyle": "light-content",
      "hidden": false,
      "translucent": true
    },
    "androidNavigationBar": {
      "backgroundColor": "#242526",
      "barStyle": "dark-content",
      "visible": true
    },
    "plugins": [
      "expo-dev-client",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#242526"
        }
      ],
      "expo-web-browser",
      "expo-font",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/notification.mp3"
          ]
        }
      ],
      "expo-video",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c99bd73f-554c-4cdf-8ecd-438221db173c"
      }
    },
    "owner": "ejjays"
  }
}
